    "typescript": "^5.3.3",
    "rollup": "^4.9.6",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "silmaril": "^0.3",
    "rollup": "^2 || ^3 || ^4"
  },
  "dependencies": {
    "unplugin-silmaril": "^0.3.3"
  },
  "scripts": {
    "prepublishOnly": "pridepack clean && pridepack build",
    "build": "pridepack build",
    "type-check": "pridepack check",
