{
  "name": "silmaril",
  "version": "0.3.3",
  "type": "module",
  "files": [
    "dist",
    "babel",
    "core"
  ],
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "keywords": [
    "pridepack"
  ],
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^20.11.10",
    "pridepack": "2.6.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "scripts": {
    "prepublishOnly": "pridepack clean && pridepack build",
    "build": "pridepack build",
    "type-check": "pridepack check",
    "clean": "pridepack clean",
    "watch": "pridepack watch",
    "start": "pridepack start",
    "dev": "pridepack dev",
    "test": "vitest"
  },
  "description": "Compile-time reactivity for JS",
  "repository": {
    "url": "https://github.com/lxsmnsyc/silmaril.git",
    "type": "git"
  },
  "homepage": "https://github.com/lxsmnsyc/silmaril/packages/silmaril",
  "bugs": {
    "url": "https://github.com/lxsmnsyc/silmaril/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Alexis Munsayac",
  "private": false,
  "dependencies": {
    "@babel/types": "^7.23.9",
    "@babel/core": "^7.23.9"
  },
  "types": "./dist/types/src/index.d.ts",
  "main": "./dist/cjs/production/index.cjs",
  "module": "./dist/esm/production/index.mjs",
  "exports": {
    "./compiler": {
      "development": {
        "require": "./dist/cjs/development/compiler.cjs",
        "import": "./dist/esm/development/compiler.mjs"
      },
      "require": "./dist/cjs/production/compiler.cjs",
      "import": "./dist/esm/production/compiler.mjs",
      "types": "./dist/types/compiler/index.d.ts"
    },
    "./store": {
      "development": {
        "require": "./dist/cjs/development/store.cjs",
        "import": "./dist/esm/development/store.mjs"
      },
      "require": "./dist/cjs/production/store.cjs",
      "import": "./dist/esm/production/store.mjs",
      "types": "./dist/types/store/index.d.ts"
    },
    ".": {
      "development": {
        "require": "./dist/cjs/development/index.cjs",
        "import": "./dist/esm/development/index.mjs"
      },
      "require": "./dist/cjs/production/index.cjs",
      "import": "./dist/esm/production/index.mjs",
      "types": "./dist/types/src/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "compiler": [
        "./dist/types/compiler/index.d.ts"
      ],
      "store": [
        "./dist/types/store/index.d.ts"
      ]
    }
  }
}
